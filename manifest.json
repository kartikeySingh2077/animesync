{
  "manifest_version": 3,
  "name": "AnimeSync",
  "version": "1.0",
  "description": "Sync your watched anime data between Hi Anime and MyAnimeList.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {}  // No icons specified
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {}  // Optionally remove this section entirely if not needed
}

